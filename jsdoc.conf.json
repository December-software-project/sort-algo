{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown", "better-docs/component", "better-docs/category"],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/better-docs"
  }
}
